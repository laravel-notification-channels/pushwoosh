{
    "name": "laravel-notification-channels/pushwoosh",
    "description": "Pushwoosh notification channel for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Choraimy Kroonstuiver",
            "email": "choraimy@live.nl",
            "homepage": "https://github.com/axlon",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/psr7": "^1.0",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1"
    },
    "require-dev": {
        "illuminate/queue": "^5.1",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^5.2 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Pushwoosh\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Pushwoosh\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Pushwoosh\\PushwooshServiceProvider"
            ]
        }
    }
}
